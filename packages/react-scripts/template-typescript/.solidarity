{
  "$schema": "http://json.schemastore.org/solidaritySchema",
  "requirements": {
    "Node": [
      {
        "rule": "cli",
        "binary": "node",
        "semver": ">=8.11.0"
      }
    ],
    "Package JSON": [
      {
        "rule": "file",
        "location": "./package.json"
      }
    ],
    "NPM": [
      {
        "rule": "cli",
        "binary": "npm",
        "semver": ">=5.6.0"
      }
    ],
    ".env": [
      {
        "rule": "file",
        "location": ".env",
        "error": "Copy .env.example to .env and add correct values."
      }
    ]
  }
}
